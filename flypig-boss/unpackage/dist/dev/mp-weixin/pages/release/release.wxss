}
/* 发布 */
.release.data-v-0766b1d4 {
	background: #C8C7CC;
	width: 100%;
	height: 100rpx;
	font-size: 40rpx;
	line-height: 100rpx;
	text-align: center;
	position: fixed;
	bottom: 0;
	left: 0;
	right: 0;
	z-index: 999;
}
.release text.data-v-0766b1d4 {
	display: block;
}
.distance.data-v-0766b1d4 {
	height: 100rpx;
}
.active.data-v-0766b1d4 {
	background: #ffdd00 !important;
}
/* 提示用户上传成功与否 */
.warp.data-v-0766b1d4 {
	position: fixed;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	background: rgba(0, 0, 0, 0.6);
	z-index: 9999;
}
.warp-view.data-v-0766b1d4 {
	width: 500rpx;
	height: 200rpx;
	background: #FFFFFF;
	margin: auto;
	position: absolute;
	-webkit-position: absolute;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	border-radius: 8rpx;
	overflow: hidden;
}
